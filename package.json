{
  "name": "entity-manager",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:standard": "next dev",
    "dev:server": "node scripts/dev-server.js",
    "build": "next build",
    "build:production": "node scripts/build.js",
    "build:preview": "npm run build && npm run start",
    "setup": "node scripts/setup.js",
    "health-check": "node scripts/health-check.js",
    "deploy": "node scripts/deploy.js",
    "export": "next build && next export",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "test": "npx vitest",
    "test:run": "npx vitest run",
    "test:coverage": "npx vitest run --coverage",
    "test:ui": "npx vitest --ui",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "clean": "rm -rf .next node_modules",
    "clean:cache": "rm -rf .next",
    "check-all": "npm run type-check && npm run lint && npm run format:check",
    "preview": "npm run build && npm run start",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:colors": "node scripts/analyze-colors.js",
    "generate:favicons": "node scripts/generate-favicons.js",
    "postinstall": "npm run health-check"
